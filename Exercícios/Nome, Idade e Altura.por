programa {
  funcao inicio() {
    cadeia nome
    escreva ("Qual é o seu nome: ")
    leia (nome)
    inteiro idade
    escreva ("Qual a sua idade: ")
    leia (idade)
    real altura
    escreva ("Qual a sua altura: ")
    leia (altura)
    escreva (nome + "\n" + "Possui: " + idade + " Anos \n" + "Possui: " + (altura) + " de altura")
  }
}
